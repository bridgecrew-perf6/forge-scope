{
  "name": "forge-scope",
  "version": "0.1.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "main": "./dist/index.js",
      "module": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "main": "./dist/*.js",
      "module": "./dist/*.js",
      "import": "./dist/*.js"
    }
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "typecheck": "tsc --noEmit --target esnext"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sambacha/forge-scope.git"
  },
  "keywords": [],
  "author": "SEE CONTRIBUTORS",
  "license": "APACHE.20 OR MIT",
  "bugs": {
    "url": "https://github.com/sambacha/forge-scope/issues"
  },
  "homepage": "https://github.com/sambacha/forge-scope#readme",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "downlevel-dts": "^0.9.0",
    "esbuild": "^0.14.37",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "tslib": "^2.3.1",
    "typescript": "4.6.3"
  },
  "dependencies": {},
  "peerDependencies": {}
}
